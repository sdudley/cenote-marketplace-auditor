{
  "name": "marketplace-auditor",
  "version": "1.0.0",
  "description": "Atlassian Marketplace transaction and license auditor",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/server/jobs/index.js",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "build": "npm run build:jobs && npm run build:express && npm run build:scripts && npm run build:client",
    "build:jobs": "tsc -p src/server/jobs/tsconfig.json",
    "build:express": "tsc -p src/server/express/tsconfig.json",
    "build:scripts": "tsc -p src/server/scripts/tsconfig.json",
    "build:client": "vite build",
    "build:client:dev": "vite build --mode development",
    "build:client:prod": "vite build --mode production",
    "start": "NODE_ENV=production node -r ./dist/express/src/server/express/ModuleResolver.js dist/express/src/server/express/index.js",
    "start:dev": "NODE_ENV=development ts-node src/server/express/index.ts",
    "jobs": "NODE_ENV=production node --enable-source-maps dist/jobs/src/server/jobs/index.js",
    "jobs:dev": "NODE_ENV=development ts-node src/server/jobs/index.ts",
    "test": "jest",
    "generate-types": "openapi-typescript https://dac-static.atlassian.com/platform/marketplace/swagger.v3.json?_v=1.2006.404 --output src/types/marketplace-api.ts",
    "add-addon": "ts-node src/server/scripts/addAddon.ts",
    "add-ignored-field": "ts-node src/server/scripts/addIgnoredField.ts",
    "backfill-version-numbers": "ts-node src/server/scripts/backfillVersionNumbers.ts",
    "export-pricing-template": "ts-node src/server/scripts/exportPricingTemplate.ts",
    "import-pricing": "ts-node src/server/scripts/importPricing.ts",
    "add-reseller": "ts-node src/server/scripts/addReseller.ts",
    "add-transaction-adjustment": "ts-node src/server/scripts/addTransactionAdjustment.ts",
    "preview:client": "vite preview",
    "preview:client:prod": "vite preview --mode production",
    "analyze:client": "vite build --mode analyze",
    "clean:client": "rimraf dist/client"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@faker-js/faker": "^9.8.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-tree-view": "^8.5.1",
    "@slack/web-api": "^7.9.2",
    "axios": "^1.6.7",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "diff": "^5.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "inversify": "^7.5.1",
    "node-cron": "^4.1.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/diff": "^5.0.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/validator": "^13.15.1",
    "@vitejs/plugin-react": "^4.5.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.6.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^6.3.5"
  }
}
