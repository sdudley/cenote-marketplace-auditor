{
  "name": "marketplace-auditor",
  "version": "1.0.0",
  "description": "Atlassian Marketplace transaction and license auditor",
  "main": "dist/server/jobrunner/index.js",
  "scripts": {
    "build": "npm run build:jobrunner && npm run build:express && npm run build:client",
    "build:jobrunner": "tsc -p src/server/jobrunner/tsconfig.json",
    "build:express": "tsc -p src/server/express/tsconfig.json",
    "build:client": "tsc -p src/client/tsconfig.json",
    "start": "ts-node src/server/jobrunner/index.ts",
    "start:jobrunner": "ts-node src/server/jobrunner/index.ts",
    "start:express": "ts-node src/server/express/index.ts",
    "start:jobrunner:js": "node --enable-source-maps dist/jobrunner/src/server/jobrunner/index.js",
    "start:express:js": "node --enable-source-maps dist/express/src/server/express/index.js",
    "dev": "concurrently \"npm run start:jobrunner\" \"npm run start:express\" \"npm run start:client\"",
    "test": "jest",
    "generate-types": "openapi-typescript https://dac-static.atlassian.com/platform/marketplace/swagger.v3.json?_v=1.2006.404 --output src/types/marketplace-api.ts",
    "add-addon": "ts-node src/server/scripts/addAddon.ts",
    "add-ignored-field": "ts-node src/server/scripts/addIgnoredField.ts",
    "backfill-version-diffs": "ts-node src/server/scripts/backfillVersionDiffs.ts",
    "backfill-version-numbers": "ts-node src/server/scripts/backfillVersionNumbers.ts",
    "export-pricing-template": "ts-node src/server/scripts/exportPricingTemplate.ts",
    "import-pricing": "ts-node src/server/scripts/importPricing.ts",
    "add-reseller": "ts-node src/server/scripts/addReseller.ts",
    "add-transaction-adjustment": "ts-node src/server/scripts/addTransactionAdjustment.ts"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "chalk": "^4.1.2",
    "csv-parse": "^5.6.0",
    "diff": "^5.1.0",
    "dotenv": "^16.4.5",
    "inversify": "^7.5.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/diff": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.6.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
